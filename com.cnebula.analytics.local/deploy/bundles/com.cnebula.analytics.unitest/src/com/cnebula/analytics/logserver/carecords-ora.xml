<?xml version="1.0" encoding="UTF-8"?>
<CALogRecord table="log_visit">
	<columns>
		<column prime="true" colName="id" name="id" type="TEXT"  length="255" nullAble="false" description="数据表主键" ></column>
		<column colName="v_id" name="vid" type="TEXT"  length="255" description="访问者唯一标识" ></column>
		<column colName="v_isnew" name="visnew" type="NUMBER"  length="1" description="新访问者标识" ></column>
		<column colName="page_url" name="url" type="TEXT"  length="255" description="页面 URL" ></column>
		<column colName="page_domain" name="domain" type="TEXT"  length="255" description="页面域" ></column>
		<column colName="page_name" name="title" type="TEXT"  length="255" description="页面名" ></column>
		<column colName="page_appid" name="appid" type="TEXT"  length="255" description="页面所属应用系统id" ></column>
		<column colName="page_appname" name="appname" type="TEXT"  length="255" description="页面所属应用系统名称" ></column>
		<column colName="log_type" name="type" type="TEXT"  length="64" description="访问记录类型" ></column>
		<column colName="pv_count" name="pv" type="NUMBER"  length="1" description="浏览量计数" ></column>
		<column colName="vv_count" name="vv" type="NUMBER"  length="1" description="访问次数计数" ></column>
		<column colName="vv_starttime_local" name="vvstl" type="DATE"  description="访问时访问者本地时间" ></column>
		<column colName="vv_exittime_local" name="vvetl" type="DATE"  description="访问者离开站点的时间" ></column>
		<column colName="vv_starttime_server" name="vvsts" type="DATE" description="访问者访问日志到达服务器的时间" ></column>
		<column colName="vv_stopped_time" name="vstopped" type="NUMBER"  length="10" description="访问者停留时间" ></column>
		<column colName="v_ip" name="vip" type="TEXT"  length="16" description="访问者IP地址" ></column>
		<column colName="v_school" name="vschool" type="TEXT"  length="128" description="访问者所属学校" ></column>
		<column colName="v_country" name="vcountry" type="TEXT"  length="2" description="访问者来自哪个国家" ></column>
		<column colName="v_province" name="vprovince" type="TEXT"  length="128" description="访问者来自哪个省" ></column>
		<column colName="v_city" name="vcity" type="TEXT"  length="128" description="访问者来自哪个市" ></column>
		<column colName="v_tenentid" name="vtid" type="TEXT"  length="64" description="访问者所属租客" ></column>
		<column colName="device_type" name="dt" type="TEXT"  length="32" description="访问者使用的设备类型" ></column>
		<column colName="device_ismobile" name="ismobile" type="NUMBER"  length="1" description="访问者使用的是否移动设备" ></column>
		<column colName="vb_os" name="vbos" type="TEXT"  length="64" description="操作系统类型" ></column>
		<column colName="vb_type" name="vbtype" type="TEXT"  length="64" description="浏览器类型" ></column>
		<column colName="vb_renderingengine" name="vbrender" type="TEXT"  length="64" description="浏览器绘图引擎" ></column>
		<column colName="vb_version" name="vbver" type="TEXT"  length="24" description="浏览器版本" ></column>
		<column colName="vb_resolution" name="vbsolution" type="TEXT"  length="24" description="浏览器分辨率" ></column>
		<column colName="vb_plugin_pdf" name="vbpPDF" type="NUMBER"  length="1" ></column>
		<column colName="vb_plugin_flash" name="vbpFLASH" type="NUMBER"  length="1" ></column>
		<column colName="vb_plugin_java" name="vbpJAVA" type="NUMBER"  length="1" ></column>
		<column colName="vb_plugin_director" name="vbpDIRECTOR" type="NUMBER"  length="1"></column>
		<column colName="vb_plugin_quicktime" name="vbpQUICKTIME" type="NUMBER"  length="1"></column>
		<column colName="vb_plugin_realplayer" name="vbpREALPLAYER" type="NUMBER"  length="1"></column>
		<column colName="vb_plugin_windowsmedia" name="vbpWINDOWSMEDIA" type="NUMBER"  length="1" ></column>
		<column colName="vb_plugin_gears" name="vbpGEARS" type="NUMBER"  length="1"></column>
		<column colName="vb_plugin_silverlight" name="vbpSILVERLIGHT" type="NUMBER"  length="1" ></column>
