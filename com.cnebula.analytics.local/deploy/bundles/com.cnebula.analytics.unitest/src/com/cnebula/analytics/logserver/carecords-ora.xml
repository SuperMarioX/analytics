		<column colName="vb_country" name="vbcountry" type="TEXT"  length="2" description="浏览器国家" ></column>
		<column colName="v_action" name="vact" type="TEXT"  length="255" description="动作" ></column>
		<column colName="v_action_time" name="vactt" type="DATE" description="动作发生时间" ></column>
		<column colName="v_action_object" name="vacto" type="TEXT"  length="255" description="动作发生对象" ></column>
		<column colName="v_action_obj_type" name="vactot" type="TEXT"  length="255" description="动作发生对象类型" ></column>
		<column colName="v_action_url" name="vactu" type="TEXT"  length="255" description="动作页面URL" ></column>
		<column colName="v_action_result" name="vactr" type="TEXT"  length="255" description="动作结果" ></column>
	</columns>
</CALogRecord>
